{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "FirebaseFileInput.tsx", "utils/authOptions.ts", "app/api/auth/[...nextauth]/route.js", "app/components/NextAuthProvider.js", "app/components/NextAuthProvider.js", "checkout.js", "pages/api/create-checkout-session.js", "backend/firebase--uploader.js"],
  "exclude": ["node_modules"]
}
